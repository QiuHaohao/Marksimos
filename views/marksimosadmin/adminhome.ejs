                                            <input type="text" ng-change="searchSeminar(searchSeminarForm)" class="form-control" id="filterKey" name="filterKey" ng-model="data.searchSeminar.filterKey" placeholder="ID / Description / Venue" ng-required="false" ng-minlength="2" ng-maxlength="20" ng-model-options="{ debounce: 500 }" />
                                        </div>
                                        <label class="control-label" ng-show="!searchSeminarForm.filterKey.$valid"><i class="fa fa-times-circle-o"></i>Search keyword length should be between 2 and 20</label>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputSeminarUserStatus" class="col-sm-2 control-label">Initialized Status</label>
                                        <div class="col-sm-4">
                                            <label class="radio-inline">
                                                <input ng-change="searchSeminar(searchSeminarForm)" type="radio" name="inputSeminarUserStatus" ng-model="data.searchSeminar.status" value="all">All
                                            </label>
                                            <label class="radio-inline">
                                                <input ng-change="searchSeminar(searchSeminarForm)" type="radio" name="inputSeminarUserStatus" ng-model="data.searchSeminar.status" value="true">Active
                                            </label>
                                            <label class="radio-inline">
                                                <input ng-change="searchSeminar(searchSeminarForm)" type="radio" name="inputSeminarUserStatus" ng-model="data.searchSeminar.status" value="false">Inactive
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="resetSeminar(searchSeminarForm)">Reset</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>

                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">Seminar List</h3>
                            </div><!-- /.box-header -->
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-striped table-hover">
                                    <tbody>
                                    <tr>
                                        <th style="width:100px">Action</th>
                                        <th>ID</th>
                                        <th>Description</th>
                                        <th>Initialized Status / Finished Status</th>
                                        <th>Country / Province / City</th>
                                        <th>Venue</th>
                                        <th>CurrentPeriod / Rounds</th>
                                        <th>Teams</th>
                                        <th>Team Member</th>
                                        <th>Add Students</th>
                                    </tr>
                                    <tr ng-repeat="seminar in data.seminars" >
                                        <td style="width:100px">
                                            <button type="button" class="btn btn-danger" ng-hide="seminar.isInitialized" ng-click="initSeminar(seminar.seminarId)" ng-disabled="css.runButtonDisabled">Init</button>

                                            <button type="button" class="btn btn-danger" ng-show="seminar.isInitialized&&!seminar.isSimulationFinished&&!css.showConfirm" ng-click="showRunSeminarConfirm(seminar.seminarId)" >Run Period{{seminar.currentPeriod}}</button>

                                            <button type="button" class="btn btn-info btn-sm" ng-show="seminar.seminarId==css.currentRunSeminarId" ng-click="runSeminar(seminar.seminarId, seminar.companyMember)" ng-disabled="css.runButtonDisabled">Confirm Yes</button>
                                            <button type="button" class="btn btn-primary btn-sm" ng-show="seminar.seminarId==css.currentRunSeminarId" ng-click="showRunSeminarConfirmNo()" >No</button>
                                        </td>
                                        <td data-highlight-key="data.searchSeminar.filterKey">{{seminar.seminarId}}
                                            <a href="adminhomereport/{{seminar.seminarId}}" target="_blank"  ng-show="seminar.isInitialized"> Report</a>
                                        </td>
                                        <td data-highlight-key="data.searchSeminar.filterKey">{{seminar.description}}</td>
                                        <td>{{seminar.isInitialized}} / {{seminar.isSimulationFinished}}</td>
                                        <td>{{seminar.country}} / {{seminar.state}} / {{seminar.city}} </td>
                                        <td data-highlight-key="data.searchSeminar.filterKey">{{seminar.venue}}</td>
                                        <td>{{seminar.currentPeriod}} / {{seminar.simulationSpan}} </td>
                                        <td>{{seminar.companyNum}} </td>
                                        <td>
                                            <p ng-repeat="company in seminar.companyAssignment">Company{{company.companyId | companyname }} :
                                                <br>
                                                <span ng-repeat="student in company.studentList">{{student}} <a href="" ng-click="removeStudentToSeminar(seminar.seminarId, student)">remove</a>, </span>
                                            </p>
                                        </td>
                                        <td style="width:460px">
                                            <div class="input-group margin">
                                                <div class="input-group-btn">
                                                    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">{{seminar.currentCompanyName}} <span class="fa fa-caret-down"></span></button>
                                                    <ul class="dropdown-menu">
                                                        <li ng-repeat="company in seminar.companies"><a href="" ng-click="chooseCompany(seminar, company)">Company {{company.companyName}}</a></li>
                                                    </ul>
                                                </div><!-- /btn-group -->
                                                <input type="email" class="form-control" placeholder="Student Email" ng-model="seminar.inputemail">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-info" type="button" ng-click="addStudentToSeminar(seminar.seminarId, seminar.inputemail)">Add</button>
                                                </span>
                                            </div><!-- /input-group -->
                                            <div class="callout callout-danger" ng-show="seminar.seminarId == css.seminarId">
                                                <p>Please choose a company and input student email !</p>
                                            </div>
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div><!-- /.box-body -->
                        </div>

                    </div>
                </div>
            </section>


            <section class="content" ng-if="css.leftmenu==52">
                <h2 class="page-header">Create New Seminar</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form" name="newSeminarForm">
                                <div class="box-body">

                                    <text-form-input type="text" name="newSeminarDescription" label="Description :" placeholder="Seminar Description" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newSeminar.description" required="true" requirederrorinfo="Description is required" minlength="2" minlengtherrorinfo="Description must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Description is a maximum of 20 characters"></text-form-input>

                                    <text-form-input type="number" name="newSeminarRounds" label="No. of Rounds :" placeholder="No. of Rounds" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newSeminar.simulation_span" min="2" minerrorinfo="Must be greater than 2" max="8" maxerrorinfo="Must be less than 8"></text-form-input>
                                    <text-form-input type="number" name="newSeminarTeams" label="No. of Teams :" placeholder="No. of Seminar Teams" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newSeminar.company_num"  min="2" minerrorinfo="Must be greater than 2" max="6" maxerrorinfo="Must be less than 6" ></text-form-input>


                                    <div class="form-group">
                                        <label for="inputSeminarCountry" class="col-sm-2 control-label">Country/State/City : </label>
                                        <div class="col-sm-3">
                                            <select class="form-control" ng-model="data.newSeminar.country" ng-options="country.name as country.name for country in data.country">

                                            </select>
                                        </div>

                                        <div class="col-sm-3">
                                            <select class="form-control">
                                                <option>Province</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-3">
                                            <select class="form-control">
                                                <option>City</option>
                                            </select>
                                        </div>
                                    </div>

                                    <text-form-input type="text" name="newSeminarVenue" label="Venue :" placeholder="Seminar Description" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newSeminar.venue" required="true" ></text-form-input>

                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="createNewSeminar(newSeminarForm)"> Create </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>

                    </div>
                </div>
            </section>








        </aside>
    </div>




    <div notifications="bottom right"></div>









