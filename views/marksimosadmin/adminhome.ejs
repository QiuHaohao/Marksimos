                                    <tbody>
                                    <tr>
                                        <th>ID</th>
                                        <th>UserName</th>
                                        <th>Email</th>
                                        <th>Status</th>
                                        <th>Licences Used / Granted</th>
                                        <th>Cell Phone</th>
                                        <th>Country / Province / City</th>
                                        <th>Address</th>
                                        <th>ID Card</th>
                                    </tr>
                                    <tr ng-repeat="user in data.distributors" >
                                        <td>{{user._id}}</td>
                                        <td>{{user.username}}</td>
                                        <td>{{user.email}}</td>
                                        <td>{{user.activated}}</td>
                                        <td>{{user.numOfUsedLicense}}/{{user.numOfLicense}}</td>
                                        <td>{{user.mobilePhone}}</td>
                                        <td>{{user.country}} / {{user.state}} / {{user.city}} </td>
                                        <td>{{user.district}} {{user.street}}</td>
                                        <td>{{user.idcardNumber}}</td>
