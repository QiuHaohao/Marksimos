<% include ./include/headcssuser.html %>

<body ng-app="marksimos">

<div class="container-fluid width_big_screen">

    <header-user login="false"></header-user>


    <div class="row login" >
        <div class="col-md-8 col-md-offset-2 panelbox">


            <div class="row">
                <div class="col-md-12"><h3>Welcome !</h3></div>
            </div>


            <div class="row contentbox">

                <div class="col-md-5 col-md-offset-7 divider_left">

                    <h4>Sign In</h4>
                    <form role="form" >
                        <div class="form-group has-success has-feedback">
                            <label for="loginInputEmail1">Email:</label>
                            <input type="email" class="form-control" id="loginInputEmail1" placeholder="Email">
                            <span class="glyphicon glyphicon-ok form-control-feedback"></span>
                        </div>
                        <div class="form-group has-error has-feedback">
                            <label for="loginInputPassword1">Password:</label>
                            <input type="password" class="form-control" id="loginInputPassword1" placeholder="Password">
                            <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                        </div>

                        <button type="submit" class="btn btn-default login_button" >Sign In</button>
                    </form>
                </div>

            </div>





        </div>

    </div>



</div>

