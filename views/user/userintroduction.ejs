<% include ./include/headcssuser.html %>

<body ng-app="marksimoslogin">

<div class="container-fluid width_big_screen" ng-controller="userIntroController" >

    <user-header showmenu="false" showlogout="true" ></user-header>


    <div class="row login" ng-if="css.showBox=='intro'" ng-cloak>
        <div class="col-md-8 col-md-offset-2 panelbox">


            <div class="row">
                <div class="col-md-12"><h3 >Introduction Videos </h3></div>
            </div>


            <div class="row video_list">
                <div class="col-md-3 col-md-offset-1">

                    <iframe width=200 height=150 src="http://player.youku.com/embed/XNzE0OTYyMTcy" frameborder=0 allowfullscreen></iframe>

                </div>

                <div class="col-md-3">

                    <iframe width=200 height=150 src="http://player.youku.com/embed/XNTY0ODgxMzAw" frameborder=0 allowfullscreen></iframe>

                </div>

                <div class="col-md-3">

                    <iframe width=200 height=150 src="http://player.youku.com/embed/XNzMxODUwODMy" frameborder=0 allowfullscreen></iframe>

                </div>
            </div>

            <div class="row">
                <div class="col-md-2 col-md-offset-9">
                    <button type="submit" class="btn btn-default intro_video_next_button" ng-click="introVideosNext()">Next</button>
                </div>
            </div>


        </div>

    </div>


    <div class="row login" ng-if="css.showBox=='seminar'" ng-cloak>
        <div class="col-md-10 col-md-offset-1 panelbox ">


            <div class="row">
                <div class="col-md-12"><h2 class="text-center game_list">List of the Game Enrolled : </h2></div>
            </div>


            <table class="table table-hover table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Seminar ID</th>
                        <th>Seminar Description</th>
                        <th>Total Round</th>
                        <th>Competitor </th>
                        <th>Date of Commencement</th>
                        <th>Seminar Status</th>
                        <!--<th>Facilitator</th>-->
                        <th>Country / Province(State) / City</th>
                        <th>Venue</th>
                    </tr>
                </thead>

                <tbody>
                    <tr ng-repeat="seminar in data.seminars">
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">{{seminar.seminarId}}   </a></td>
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">{{seminar.descriptiondescription}} </a></td>
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">{{seminar.simulationSpan}}</a></td>
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">{{seminar.companyNum}} </a></td>
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">no info  </a></td>
                        <td> <a href="" ng-click="chooseSeminar(seminar.seminarId )" ng-if="seminar.isInitialized">Initialized, Current Round:{{seminar.currentPeriod}} </a>
                            <a href="" ng-click="chooseSeminar(seminar.seminarId )" ng-if="!seminar.isInitialized">Not Initialized </a>
                        </td>
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">{{seminar.country}} / {{seminar.state}} / {{seminar.city}}  </a></td>
                        <td><a href="" ng-click="chooseSeminar(seminar.seminarId )">{{seminar.venue}} </a></td>
                    </tr>
                </tbody>
            </table>

        </div>
    </div>


    <div class="row login" ng-if="css.showBox=='whoami'" ng-cloak>
        <div class="col-md-8 col-md-offset-2 panelbox bg_whoami">

            <div class="row">
                <div class="col-md-12"><h2 class="text-center first_intro">Who am I ? </h2></div>
            </div>


            <div class="row whoami">
                <div class="col-md-8 col-md-offset-2">

                    <div class="row ">
                        <div class="col-md-12">

                            <h5 class="text-center">I am leading <span class="highlight1">{{data.currentStudentSeminar.companyName}}</span> .
                                I manage <span class="highlight2">{{data.skuNumber}}</span>  <span class="highlight1">products</span> under <span class="highlight2">{{data.brandNumber}}</span>  <span class="highlight1">brands</span> .
                                <br/> I compete with <span class="highlight2">{{data.currentStudentSeminar.numOfCompany - 1 }}</span>  other companies.
                            </h5>

                        </div>
                    </div>

                    <div class="row ">
                        <div class="col-md-12">

                            <h5 class="text-center">My companyâ€™s mission is to get the <span class="highlight1">highest Market Share</span>  and <span class="highlight1">Profit</span> !</h5>

                        </div>
                    </div>

                </div>
            </div>


            <div class="row brand_list">
                <div class="row">
                    <div class="col-md-2" ng-class="{'col-md-offset-1':$first, 'user_brandcolumn':!$first,}" ng-repeat="brand in data.currentCompany.d_BrandsDecisions">
                        <span class="user_brand">{{brand.d_BrandName}}</span>
                        <ul>
                            <li class="user_product" ng-repeat="sku in brand.d_SKUsDecisions">{{sku.d_SKUName}} - <span>{{sku.d_TargetConsumerSegment | usersegment}}</span></li>
                        </ul>
                    </div>
                </div>


                <div class="row">
                    <div class="col-md-2 col-md-offset-9">
                        <button type="submit" class="btn btn-default start_button" ng-click="whoamiNext()">Start</button>
                    </div>
                </div>

            </div>
        </div>

    </div>


</div>

<% include ./include/footerscriptuserlogin.html %>

