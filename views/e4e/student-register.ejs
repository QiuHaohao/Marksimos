<% include ./include/registerheader.html %>
<body ng-app='e4eregister'>

	<div id="header" class="white-header" ng-controller="registerHeaderController">
		<div class="wrap">
			<div class="e4e-logo">
				<a href="#">
					<img src="/appe4e/img/LOGO-E4E-white.png" alt="">
				</a>
			</div>
			<div class="learning-logo">
				<a href="/">
					<img src="/appe4e/img/LOGO-HCDLEARNING-white.png" alt="">
				</a>
			</div>
			<div class="languages">
				<nav class="nav-header">
					<ul>
						<li ng-class="{'active':!isEnglish}">
							<a href='' class="f-bold" ng-click="changeLanguage('zh_CN')">中</a>
						</li>
						<li ng-class="{'active':isEnglish}">
							<a href='' class="f-bold" ng-click="changeLanguage('en_US')">EN</a>
						</li>
					</ul>
				</nav>
			</div>
		</div>
	</div>
	<div id="footer">
		<div class="wrap">
			<span class="col-lg-6 text-left">© HCD Learning Ltd. 沪ICP 10011451号</span>
			<span class="col-lg-6 text-right">HCD Learning 合得软件</span>
		</div>
	</div>

	<div id="student-register" ng-controller='studentRegisterController'>
		<form name="studentRegisterForm" role="form">
			<div class="section section-red" id="section0">
				<div>
					<div class="register-title">
						<h3 class="f-semibold text-center f-size-26">{{'e4eStudentRegisterTitle' | translate }}</h3>
					</div>
					<div class="student-back-1">
						<h4 class="f-semibold text-center f-size-20 padding-10">{{'e4eStudentFirstPageTitle' | translate }}</h4>
						<div class="form-horizontal">
							<div class="form-group">
							    <label for="user-name" class="col-sm-3-5 control-label">{{'e4eStudentUserName' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="user-name" ng-model='userName' placeholder="xxxxxxx">
							    </div>
							</div>

							<div class="form-group">
							    <label for="first-name" class="col-sm-3-5 control-label">{{'e4eStudentFirstName' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="first-name" ng-model='firstName' placeholder="xxxxxxx">
							    </div>
							</div>
							<div class="form-group">
							    <label for="last-name" class="col-sm-3-5 control-label">{{'e4eStudentLastName' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="last-name" ng-model='lastName' placeholder="xxxxxxx">
							    </div>
							</div>

							<div class="form-group">
							    <label for="age" class="col-sm-3-5 control-label">{{'e4eStudentAge' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="age" ng-model='age' placeholder="xxxxxxx">
							    </div>
							</div>

							<div class="form-group">
							    <label for="country" class="col-sm-3-5 control-label">{{'e4eStudentCountry' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="country" ng-model='country' placeholder="xxxxxxx">
							    </div>
							</div>

							<div class="form-group">
							    <label for="address" class="col-sm-3-5 control-label">{{'e4eStudentAddress' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="address" ng-model='address' placeholder="xxxxxxx">
							    </div>
							</div>

							<h4 class="f-semibold text-center f-size-20 append-bottom-24">{{'e4eStudentCommDetail' | translate }}</h4>

							<div class="form-group">
							    <label for="email" class="col-sm-3-5 control-label">{{'e4eStudentEmail' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="email" ng-model='email' placeholder="xxxxxxx">
							    </div>
							</div>

							<div class="form-group">
							    <label for="qq" class="col-sm-3-5 control-label">{{'e4eStudentQQ' | translate }}</label>
							    <div class="col-sm-4-2">
							    	<input type="text" class="form-control input-sm" id="qq" ng-model='qq' placeholder="xxxxxxx">
							    </div>
							</div>

							<div class="form-group">
							    <div class="col-sm-offset-3-5 col-sm-4-2">
							    	<button type="text" class="btn btn-default next">{{'e4eCompanyNext' | translate }}</button>
							    </div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="section section-red" id="section1">
				<div>
					<div class="register-title">
						<h3 class="f-semibold text-center f-size-26">{{'e4eStudentRegisterTitle' | translate }}</h3>
					</div>
					<div class="student-back-2">
						<h4 class="f-semibold text-center f-size-20 padding-10">{{'e4eStudentSecondPageTitle' | translate }}</h4>
						<div class="form-horizontal">
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="highestEDU" ng-model='highestEDU' placeholder="{{'e4eStudentHighestEDU' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="marjors" ng-model='marjors' placeholder="{{'e4eStudentMajors' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="university" ng-model='university' placeholder="{{'e4eStudentUniversity' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="scoreGPA" ng-model='scoreGPA' placeholder="{{'e4eStudentScoreGPA' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="achievements" ng-model='achievements' placeholder="{{'e4eStudentAchievements' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="year-of-graduation" ng-model='yearOfGraduation' placeholder="{{'e4eStudentYearOfGraduation' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="month-of-graduation" ng-model='monthOfGraduation' placeholder="{{'e4eStudentMonthOfGraduation' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="current-year" ng-model='currentYear' placeholder="{{'e4eStudentCurrentYear' | translate }}">
							    </div>
							</div>
							<div class="form-group">
								<div class="col-sm-offset-3 col-sm-6">
									<input type="text" class="form-control input-sm" id="upload-resume" ng-model='uploadResume' placeholder="{{'e4eStudentUploadResume' | translate }}">
							    </div>
							</div>

							<div class="form-group">
						    	<div class="col-sm-offset-3 col-sm-6">
						    		<button class="btn btn-default" ng-click='submitRegister()'>{{'e4eStudentSubmit' | translate }}</button>
						    	</div>
						    </div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
<% include ./include/registerfooter.html %>
