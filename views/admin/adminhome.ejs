                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputDistributorPassword" class="col-sm-4 control-label">Password</label>
                                    <div class="col-sm-8">
                                        <input type="password" class="form-control" id="inputDistributorPassword" placeholder="Password">
                                    </div>
                                </div>


                                <div class="form-group">

                                    <div class="col-sm-offset-4 col-sm-2">
                                        <select class="form-control input-sm" ng-model="data.newDistributor.country" ng-options="country.name for country in data.country">

                                        </select>
                                    </div>

                                    <div class="col-sm-2">
                                        <select class="form-control input-sm">
                                            <option>Province</option>
                                        </select>
                                    </div>

                                    <div class="col-sm-3">
                                        <input type="text" class="form-control" id="inputDistributorCity" placeholder="City">
                                    </div>
                                </div>


                                <div class="form-group">
                                    <label for="inputDistributorLicence" class="col-sm-4 control-label">No. of Licences</label>
                                    <div class="col-sm-8">
                                        <input type="number" class="form-control" id="inputDistributorLicence" placeholder="pls input licence">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="col-sm-offset-4 col-sm-10">
                                        <button type="submit" class="btn btn-primary">Search</button>
                                    </div>
                                </div>
                            </form>
                        </div>

