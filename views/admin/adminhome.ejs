                                    <licence-input labeltext="No. of Licences Granted :" placeholdertext="No. of Licences Granted" formname="newFacilitatorForm" data="data.newFacilitator.num_of_license_granted" required="false" requirederrorinfo="ID Card is required" min="1" minerrorinfo="Must be greater than 1" max="200" maxerrorinfo="Must be less than 200"></licence-input>


                                    <!--<div class="form-group">-->
                                    <!--<label for="inputFacilitatorLicenceGranted" class="col-sm-2 control-label">Game Type :</label>-->
