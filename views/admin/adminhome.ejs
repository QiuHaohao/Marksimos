                                    <!--<option>MARKSIMOS B2C</option>-->
                                    <!--<option>CASASIMOS</option>-->
                                    <!--</select>-->
                                    <!--</div>-->
                                    <!--</div>-->
                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="createNewFacilitator(newFacilitatorForm)"> Create </button>
