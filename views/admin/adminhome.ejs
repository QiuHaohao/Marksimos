<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>MarkSimos - User Home</title>
    <!-- You'll see I've linked the screen.css generated by Compass -->

    <!-- Compass reset  -->
    <!--<link rel="stylesheet" href="app/css/stylesheets/screen.css">-->

    <!-- Bootstrap -->
    <link rel="stylesheet" href="libs/bootstrap/dist/css/bootstrap.css">
    <!--<link rel="stylesheet" href="libs/bootstrap/dist/css/bootstrap-theme.css">-->

    <link rel="stylesheet" href="app/css/stylesheets/main.css">


    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>


<body ng-app="marksimosadmin">

<div class="container-fluid mainbox" ng-controller="adminLoginController">

    <div admin-header></div>

    <div class="row">

        <div class="col-md-2">

            <ul class="nav nav-pills nav-stacked">
                <li ng-class="{'active':css.leftmenu=='distributor'}"><a href="#" ng-click="css.leftmenu='distributor'">Distributor Management</a></li>
                <li ng-class="{'active':css.leftmenu=='facilitator'}"><a href="#" ng-click="css.leftmenu='facilitator'">Facilitator Management</a></li>
                <li ng-class="{'active':css.leftmenu=='seminar'}"><a href="#" ng-click="css.leftmenu='seminar'">Seminar Management</a></li>
            </ul>
        </div>

        <div class="col-md-10" ng-switch on="css.leftmenu" >

            <div class="row" ng-switch-when="distributor" >

                <div class="row">
                    <div class="col-md-12">

                        <div class="page-header">
                            <h3>Distributor Management <small> Help</small></h3>
                        </div>

                    </div>
                </div>

                <div class="row" ng-hide="css.updatestatus">
                    <div class="col-md-8">
                        <div class="page-header">
                            <h4>Create New Distributor </h4>
                        </div>

                        <form class="form-horizontal" role="form">

                            <div class="form-group">
                                <label for="inputDistributorName" class="col-sm-4 control-label">Distributor Name</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="inputDistributorName" placeholder="Distributor Name">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputDistributorEmail" class="col-sm-4 control-label">Distributor Email</label>
                                <div class="col-sm-8">
                                    <input type="email" class="form-control" id="inputDistributorEmail" placeholder="Email">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputDistributorPassword" class="col-sm-4 control-label">Password</label>
                                <div class="col-sm-8">
                                    <input type="password" class="form-control" id="inputDistributorPassword" placeholder="Password">
                                </div>
                            </div>


                            <div class="form-group">

                                <div class="col-sm-offset-4 col-sm-2">
                                    <select class="form-control input-sm" ng-model="data.newDistributor.country" ng-options="country.name for country in data.country">

                                    </select>
                                </div>

                                <div class="col-sm-2">
                                    <select class="form-control input-sm">
                                        <option>Province</option>
                                    </select>
                                </div>

                                <div class="col-sm-3">
                                    <input type="text" class="form-control" id="inputDistributorCity" placeholder="City">
                                </div>
                            </div>


                            <div class="form-group">
                                <label for="inputDistributorLicence" class="col-sm-4 control-label">No. of Licences</label>
                                <div class="col-sm-8">
                                    <input type="number" class="form-control" id="inputDistributorLicence" placeholder="pls input licence">
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-10">
                                    <button type="submit" class="btn btn-primary">Create New Distributor</button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>

                <div class="row" ng-hide="css.updatestatus">
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>Country / Province / City</th>
                                        <th>Licence</th>
                                    </tr>
                                </thead>

                                <tbody>
                                    <tr ng-repeat="user in data.distributors" ng-click="css.updatestatus='true'">
                                        <td>{{user.id}}</td>
                                        <td>{{user.name}}</td>
                                        <td>{{user.email}}</td>
                                        <td>China / Shanghai / Shanghai</td>
                                        <td>{{user.licence}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>


                <div class="row" ng-show="css.updatestatus">
                    <div class="col-md-8">

                        <div class="page-header">
                            <h4>Modify Distributor XXX</h4>
                        </div>

                        <form class="form-horizontal" role="form">

                            <div class="form-group">
                                <label for="inputDistributorName" class="col-sm-4 control-label">Distributor Name</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="inputDistributorName" placeholder="Distributor Name">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputDistributorEmail" class="col-sm-4 control-label">Distributor Email</label>
                                <div class="col-sm-8">
                                    <input type="email" class="form-control" id="inputDistributorEmail" placeholder="Email">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputDistributorPassword" class="col-sm-4 control-label">Password</label>
                                <div class="col-sm-8">
                                    <input type="nu" class="form-control" id="inputDistributorPassword" placeholder="Password">
                                </div>
                            </div>


                            <div class="form-group">

                                <div class="col-sm-offset-4 col-sm-2">
                                    <select class="form-control input-sm" ng-model="data.newDistributor.country" ng-options="country.name for country in data.country">

                                    </select>
                                </div>

                                <div class="col-sm-2">
                                    <select class="form-control input-sm">
                                        <option>Province</option>
                                    </select>
                                </div>

                                <div class="col-sm-3">
                                    <input type="text" class="form-control" id="inputDistributorCity" placeholder="City">
                                </div>
                            </div>


                            <div class="form-group">
                                <label for="inputDistributorLicence" class="col-sm-4 control-label">No. of Licences</label>
                                <div class="col-sm-8">
                                    <input type="number" class="form-control" id="inputDistributorLicence" placeholder="pls input licence">
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-10">
                                    <button type="submit" class="btn btn-primary">Save</button>
                                    <button type="submit" class="btn btn-default" ng-click="css.updatestatus='false'">Cancel</button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>

            </div>
            <!---- END Distributor ---->





            <div class="row" ng-switch-when="facilitator" >

                <div class="row">
                    <div class="col-md-12">

                        <div class="page-header">
                            <h3>Facilitator Management <small> Help</small></h3>
                        </div>

                    </div>
                </div>

                <div class="row" ng-hide="css.updatestatus">
                    <div class="col-md-8">
                        <div class="page-header">
                            <h4>Create New Facilitator </h4>
                        </div>

                        <form class="form-horizontal" role="form">

                            <div class="form-group">
                                <label for="inputFacilitatorName" class="col-sm-4 control-label">Facilitator Name</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="inputFacilitatorName" placeholder="Facilitator Name">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputFacilitatorEmail" class="col-sm-4 control-label">Facilitator Email</label>
                                <div class="col-sm-8">
                                    <input type="email" class="form-control" id="inputFacilitatorEmail" placeholder="Email">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputFacilitatorPassword" class="col-sm-4 control-label">Password</label>
                                <div class="col-sm-8">
                                    <input type="password" class="form-control" id="inputFacilitatorPassword" placeholder="Password">
                                </div>
                            </div>


                            <div class="form-group">

                                <div class="col-sm-offset-4 col-sm-2">
                                    <select class="form-control input-sm" ng-model="data.newFacilitator.country" ng-options="country.name for country in data.country">
                                    </select>
                                </div>

                                <div class="col-sm-2">
                                    <select class="form-control input-sm">
                                        <option>Province</option>
                                    </select>
                                </div>

                                <div class="col-sm-3">
                                    <input type="text" class="form-control" id="inputFacilitatorCity" placeholder="City">
                                </div>
                            </div>


                            <div class="form-group">
                                <label for="inputFacilitatorLicence" class="col-sm-4 control-label">No. of Licences</label>
                                <div class="col-sm-8">
                                    <input type="number" class="form-control" id="inputFacilitatorLicence" placeholder="pls input licence">
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-10">
                                    <button type="submit" class="btn btn-primary">Create New Facilitator</button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>

                <div class="row" ng-hide="css.updatestatus">
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped table-hover">
                                <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Country / Province / City</th>
                                    <th>Licence</th>
                                    <th>Belong to Distributor</th>
                                </tr>
                                </thead>

                                <tbody>
                                <tr ng-repeat="user in data.facilitators" ng-click="css.updatestatus='true'">
                                    <td>{{user.id}}</td>
                                    <td>{{user.name}}</td>
                                    <td>{{user.email}}</td>
                                    <td>China / Shanghai / Shanghai</td>
                                    <td>{{user.licence}}</td>
                                    <td>{{user.distributor}}</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>


                <div class="row" ng-show="css.updatestatus">
                    <div class="col-md-8">

                        <div class="page-header">
                            <h4>Modify Facilitator XXX</h4>
                        </div>

                        <form class="form-horizontal" role="form">

                            <div class="form-group">
                                <label for="inputFacilitatorName" class="col-sm-4 control-label">Distributor Name</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="inputFacilitatorName" placeholder="Distributor Name">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputFacilitatorEmail" class="col-sm-4 control-label">Distributor Email</label>
                                <div class="col-sm-8">
                                    <input type="email" class="form-control" id="inputFacilitatorEmail" placeholder="Email">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="inputFacilitatorPassword" class="col-sm-4 control-label">Password</label>
                                <div class="col-sm-8">
                                    <input type="password" class="form-control" id="inputFacilitatorPassword" placeholder="Password">
                                </div>
                            </div>


                            <div class="form-group">

                                <div class="col-sm-offset-4 col-sm-2">
                                    <select class="form-control input-sm" ng-model="data.newFacilitator.country" ng-options="country.name for country in data.country">

                                    </select>
                                </div>

                                <div class="col-sm-2">
                                    <select class="form-control input-sm">
                                        <option>Province</option>
                                    </select>
                                </div>

                                <div class="col-sm-3">
                                    <input type="text" class="form-control" id="inputFacilitatorCity" placeholder="City">
                                </div>
                            </div>


                            <div class="form-group">
                                <label for="inputFacilitatorLicence" class="col-sm-4 control-label">No. of Licences</label>
                                <div class="col-sm-8">
                                    <input type="number" class="form-control" id="inputFacilitatorLicence" placeholder="pls input licence">
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-10">
                                    <button type="submit" class="btn btn-primary">Save</button>
                                    <button type="submit" class="btn btn-default" ng-click="css.updatestatus='false'">Cancel</button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>

            </div>
            <!-- END Facilitator -->

        </div>

    </div>


</div>



<script src="libs/jquery/dist/jquery.js"></script>
<script src="libs/angular/angular.js"></script>
<script src="libs/d3/d3.js"></script>
<script src="libs/angular-charts/dist/angular-charts.js"></script>

<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="libs/bootstrap/dist/js/bootstrap.js"></script>


<script src="app/js/adminapp.js"></script>



</body>
</html>