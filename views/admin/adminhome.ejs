                                        <td>{{seminar.seminarId}}</td>
                                        <td>{{seminar.description}}</td>
                                        <td>{{seminar.isInitialized}} </td>
                                        <td>{{seminar.isSimulationFinised}} </td>
                                        <td>{{seminar.country}} / {{seminar.state}} / {{seminar.city}} </td>
                                        <td>{{seminar.venue}}</td>
                                        <td>{{seminar.currentPeriod}} / {{seminar.simulationSpan}} </td>
                                        <td>{{seminar.companyNum}} </td>
                                        <td>
                                            <button class="btn btn-primary btn-flat" type="button" ng-click="initSeminar(seminar.seminarId)">Init</button>
                                            <button class="btn btn-primary btn-flat" type="button" ng-click="runSeminar(seminar.seminarId)">Run</button>
                                        </td>
                                        <td>
                                            <div class="row">
                                                <div class="col-lg-5">
                                                    <div class="input-group margin">
                                                        <div class="input-group-btn">
                                                            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">{{data.addStudentToSeminar.companyName}} <span class="fa fa-caret-down"></span></button>
                                                            <ul class="dropdown-menu">
                                                                <li ng-repeat="company in seminar.companies"><a href="" ng-click="chooseCompany(seminar, company)">{{company.companyName}}</a></li>
                                                            </ul>
                                                        </div><!-- /btn-group -->
                                                        <input type="text" class="form-control" ng-model="data.addStudentToSeminar.student_id">
                                                        <span class="input-group-btn">
                                                            <button class="btn btn-info btn-flat" type="button" ng-click="addStudentToSeminar()">Add</button>
                                                        </span>
                                                    </div><!-- /input-group -->
                                                </div>
                                            </div>
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div><!-- /.box-body -->
                        </div>

                    </div>
                </div>
            </section>


