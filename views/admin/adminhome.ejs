
<% include ./include/headcssadmin.html %>


<body class="skin-blue" ng-app="marksimosadmin" ng-controller="adminHomeController">

    <header-admin class="header"></header-admin>


    <div class="wrapper row-offcanvas row-offcanvas-left">

        <menu-admin current-menu="css.leftmenu" showmenu="css.menuShow"></menu-admin>


        <aside class="right-side">

            <section class="content" ng-if="css.leftmenu==21">
                <h2 class="page-header">Distributor Management</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form" name="searchDistributorForm">
                                <div class="box-body">

                                    <username-input labeltext="Distributor UserName :" placeholdertext="Distributor UserName" formname="searchDistributorForm" data="data.searchDistributor.username" required="false" requirederrorinfo="Username is required" minlength="6" minlengtherrorinfo="Username must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Username is a maximum of 20 characters"></username-input>
                                    <email-input labeltext="Distributor Email :" placeholdertext="Email" formname="searchDistributorForm" data="data.searchDistributor.email" required="false" requirederrorinfo="Email is required" emailerrorinfo="Doesn't look like a valid email !"></email-input>


                                    <div class="form-group">
                                        <label for="inputDistributorUserStatus" class="col-sm-2 control-label">User Status</label>
                                        <div class="col-sm-4">
                                            <label class="radio-inline">
                                                <input type="radio" name="inputDistributorUserStatus" ng-model="data.searchDistributor.user_status" value="false">Active
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="inputDistributorUserStatus" ng-model="data.searchDistributor.user_status" value="true">Inactive
                                            </label>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="searchDistributor(searchDistributorForm)">Search</button>
                                        </div>
                                    </div>
                                </div>

                            </form>
                        </div>

                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">Distributor List</h3>
                            </div><!-- /.box-header -->
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-striped table-hover">
                                    <tbody>
                                    <tr>
                                        <th>ID</th>
                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>Status</th>
                                        <th>Licences Used / Granted</th>
                                        <th>Cell Phone</th>
                                        <th>Country / Province / City</th>
                                        <th>Address</th>
                                        <th>ID Card</th>
                                    </tr>
                                    <tr ng-repeat="user in data.distributors" >
                                        <td>{{user._id}}</td>
                                        <td>{{user.name}}</td>
                                        <td>{{user.email}}</td>
                                        <td>{{!user.isDisabled}}</td>
                                        <td>{{user.numOfUsedLicense}}/{{user.numOfLicense}}</td>
                                        <td>{{user.phone}}</td>
                                        <td>{{user.country}} / {{user.state}} / {{user.city}} </td>
                                        <td>{{user.district}} {{user.street}}</td>
                                        <td>{{user.pincode}}</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div><!-- /.box-body -->
                        </div>

                    </div>
                </div>
            </section>


            <section class="content" ng-if="css.leftmenu==22">
                <h2 class="page-header">Create New Distributor</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form" name="newDistributorForm">
                                <div class="box-body">

                                    <username-input labeltext="Distributor UserName :" placeholdertext="Distributor UserName" formname="newDistributorForm" data="data.newDistributor.username" required="true" requirederrorinfo="Username is required" minlength="6" minlengtherrorinfo="Username must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Username is a maximum of 20 characters"></username-input>
                                    <email-input labeltext="Distributor Email :" placeholdertext="Email" formname="newDistributorForm" data="data.newDistributor.email" required="true" requirederrorinfo="Email is required" emailerrorinfo="Doesn't look like a valid email !"></email-input>
                                    <password-input labeltext="Password :" placeholdertext="Password" formname="newDistributorForm" data="data.newDistributor.password" required="true" requirederrorinfo="Password is required" minlength="6" minlengtherrorinfo="Password must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Password is a maximum of 20 characters"></password-input>
                                    <phone-input labeltext="Phone :" placeholdertext="Cell Phone" formname="newDistributorForm" data="data.newDistributor.phone" required="false" requirederrorinfo="Phone is required" minlength="11" minlengtherrorinfo="Phone must be at least 11 characters" maxlength="15" maxlengtherrorinfo="Phone is a maximum of 15 characters"></phone-input>
                                    <idcard-input labeltext="ID Card :" placeholdertext="Pin Code" formname="newDistributorForm" data="data.newDistributor.pincode" required="false" requirederrorinfo="ID Card is required" minlength="17" minlengtherrorinfo="ID Card must be at least 17 characters" maxlength="24" maxlengtherrorinfo="ID Card is a maximum of 24 characters"></idcard-input>

                                    <div class="form-group">
                                        <label for="inputDistributorCountry" class="col-sm-2 control-label">Country/State/City : </label>
                                        <div class="col-sm-3">
                                            <select class="form-control" ng-model="data.newDistributor.country" ng-options="country.name as country.name for country in data.country">

                                            </select>
                                        </div>

                                        <div class="col-sm-3">
                                            <select class="form-control">
                                                <option>Province</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-3">
                                            <select class="form-control">
                                                <option>City</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="inputDistributorAddress" class="col-sm-2 control-label">Address : </label>
                                        <div class="col-sm-3">
                                            <input type="text" class="form-control" id="inputDistributorAddress" placeholder="Address - District" ng-model="data.newDistributor.district">
                                        </div>
                                        <div class="col-sm-3">
                                            <input type="text" class="form-control" id="inputDistributorAddressStreet" placeholder="Address - Street/Building" ng-model="data.newDistributor.street">
                                        </div>
                                    </div>

                                    <licence-input labeltext="No. of Licences Granted :" placeholdertext="No. of Licences Granted" formname="newDistributorForm" data="data.newDistributor.num_of_license_granted" required="false" requirederrorinfo="ID Card is required" min="1" minerrorinfo="Must be greater than 1" max="200" maxerrorinfo="Must be less than 200"></licence-input>


                                    <!--<div class="form-group">-->
                                    <!--<label for="inputDistributorLicenceGranted" class="col-sm-2 control-label">Game Type :</label>-->
                                    <!--<div class="col-sm-4">-->
                                    <!--<select class="form-control input-sm">-->
                                    <!--<option>MARKSIMOS B2B</option>-->
                                    <!--<option>MARKSIMOS B2C</option>-->
                                    <!--<option>CASASIMOS</option>-->
                                    <!--</select>-->
                                    <!--</div>-->
                                    <!--</div>-->
                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="createNewDistributor(newDistributorForm)"> Create </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>



                    </div>
                </div>
            </section>


            <section class="content" ng-if="css.leftmenu==31">
                <h2 class="page-header">Facilitator Management</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form" name="searchFacilitatorForm">
                                <div class="box-body">

                                    <username-input labeltext="Facilitator UserName :" placeholdertext="Facilitator UserName" formname="searchFacilitatorForm" data="data.searchFacilitator.username" required="false" requirederrorinfo="Username is required" minlength="6" minlengtherrorinfo="Username must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Username is a maximum of 20 characters"></username-input>
                                    <email-input labeltext="Facilitator Email :" placeholdertext="Email" formname="searchFacilitatorForm" data="data.searchFacilitator.email" required="false" requirederrorinfo="Email is required" emailerrorinfo="Doesn't look like a valid email !"></email-input>


                                    <div class="form-group">
                                        <label for="inputFacilitatorUserStatus" class="col-sm-2 control-label">User Status</label>
                                        <div class="col-sm-4">
                                            <label class="radio-inline">
                                                <input type="radio" name="inputFacilitatorUserStatus" ng-model="data.searchFacilitator.user_status" value="false">Active
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="inputFacilitatorUserStatus" ng-model="data.searchFacilitator.user_status" value="true">Inactive
                                            </label>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="searchFacilitator(searchFacilitatorForm)">Search</button>
                                        </div>
                                    </div>
                                </div>

                            </form>
                        </div>

                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">Facilitator List</h3>
                            </div><!-- /.box-header -->
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-striped table-hover">
                                    <tbody>
                                    <tr>
                                        <th>ID</th>
                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>Status</th>
                                        <th>Licences Used / Granted</th>
                                        <th>Cell Phone</th>
                                        <th>Country / Province / City</th>
                                        <th>Address</th>
                                        <th>ID Card</th>
                                        <th>Distributor</th>
                                    </tr>
                                    <tr ng-repeat="user in data.facilitators" >
                                        <td>{{user._id}}</td>
                                        <td>{{user.name}}</td>
                                        <td>{{user.email}}</td>
                                        <td>{{!user.isDisabled}}</td>
                                        <td>{{user.numOfUsedLicense}}/{{user.numOfLicense}}</td>
                                        <td>{{user.phone}}</td>
                                        <td>{{user.country}} / {{user.state}} / {{user.city}} </td>
                                        <td>{{user.district}} {{user.street}}</td>
                                        <td>{{user.pincode}}</td>
                                        <td title="{{user.distributorId}}">{{user.distributorId}}</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div><!-- /.box-body -->
                        </div>

                    </div>
                </div>
            </section>


            <section class="content" ng-if="css.leftmenu==32">
                <h2 class="page-header">Create New Facilitator</h2>

