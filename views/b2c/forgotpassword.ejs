<% include include/headcss.ejs %>


<body ng-app="b2clogin">

    <b2c-header ></b2c-header>


    <div class="b2c-top-line">
      <div class="b2c-container">
          <label><i class="glyphicon glyphicon-lock"></i>安全中心</label> 
          <span>找回密码</span>
          <a href="#">输入邮箱</a>
            &gt;
          <a href="#" class="active">重设密码</a>
            &gt;
          <a href="#">完成</a>
      </div>
    </div>


    <main>
        <div class="middle b2c-container">                       
              <div class="b2c-enter-email">
                  <div class="row" >
                      <div class="col-md-5">
                          <div class="b2c-prints">

                          </div>
                      </div>
                      <div class="col-md-5">
                            <h3><strong>输入你的注册邮箱：</strong></h3>
                            <div class="form-group"><input class=" form-control" type="text" /></div>
                          <p><button class="btn btn-default">找回密码</button></p>
                      </div>
                     
                  </div>
                  <hr/>
                  <div class="text-center tip">
                      <h3><strong> 没有收到邮件？</strong></h3>
                      <div class="text-left">
                          &gt;<span>查看邮箱回收站、垃圾邮件</span>
                          <br/>
                          &gt;<a class="text-danger">重新发送邮件</a>
                      </div>                      
                  </div>
              </div>        
        </div>   
    </main>


