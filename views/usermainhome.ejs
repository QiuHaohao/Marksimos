    </div>


    <div class="row mainbody" ng-if="css.menu=='Home'">
        <div class="col-md-12">
            <div class="row user_home_divider_horizontal ">
                <div class="col-md-6 col-md-offset-3 ">
                    <img src="./app/css/images/temp1.png" alt=""/>
                </div>
            </div>


            <div class="row">
                <div class="col-md-6 col-md-offset-3 user_search_chart">
                    <div class="input-group">
                        <div class="input-group-btn">

                            <button type="button" class="btn btn-default dropdown-toggle droplist_company" data-toggle="dropdown">Company A <span class="caret"></span></button>
                            <ul class="dropdown-menu">
                                <li><a href="#">Company A</a></li>
                                <li><a href="#">Company B</a></li>
                                <li><a href="#">Company C</a></li>
                            </ul>
                        </div><!-- /btn-group -->

                        <input type="text" class="form-control" placeholder="find report or chart">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button"><span class="glyphicon glyphicon-search"></span></button>
                        </span>
                    </div><!-- /input-group -->
                </div>
            </div>


            <div class="row">
                <div class="col-md-3 col-md-offset-1">
                    <div class="panel panel-primary user_chart_list">
                        <div class="panel-heading">
                            <h3 class="panel-title">Your Company</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item" ng-click="clickChartMenu('A1')">Company Status</li>
                            <li class="list-group-item list-group-item-active" ng-click="clickChartMenu('A2')">Financial Data</li>
                            <li class="list-group-item" ng-click="clickChartMenu('A3')">Inventory Report</li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 ">
                    <div class="panel panel-primary user_chart_list user_chart_list_bg2">
                        <div class="panel-heading">
                            <h3 class="panel-title">Your Competitor</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item" ng-click="clickChartMenu('B1')">Market Share</li>
                            <li class="list-group-item list-group-item-active" ng-click="clickChartMenu('B2')">Competitor Information</li>
                            <li class="list-group-item" ng-click="clickChartMenu('B3')">Investment & Profits</li>
                            <li class="list-group-item list-group-item-active" ng-click="clickChartMenu('B4')">Market Sales & Inventory</li>

                        </ul>
                    </div>
                </div>

                <div class="col-md-3 ">
                    <div class="panel panel-primary user_chart_list">
                        <div class="panel-heading">
                            <h3 class="panel-title">Market Landscape</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item" ng-click="clickChartMenu('C1')">Segment Leader Top5</li>
                            <li class="list-group-item list-group-item-active" ng-click="clickChartMenu('C2')">Perception Map</li>
                            <li class="list-group-item" ng-click="clickChartMenu('C3')">Segment Distributions</li>
                            <li class="list-group-item list-group-item-active" ng-click="clickChartMenu('C4')">Market Evolution</li>
                            <li class="list-group-item" ng-click="clickChartMenu('C5')">Market Indicator</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>







    <div class="row mainbody bgbrand" ng-if="css.menu=='Report'">
        <div class="col-md-2 user_home_divider user_chart_box">

            <div class="row ">
                <h4 class="user_maincompany user_chart_company">Company <span class="company_a">A</span> </h4>

            </div>

        </div>

        <div class="col-md-5 user_home_divider user_chart_box" >
            <div class="row">
                <div class="col-md-12 user_search_chart user_chart_search">
                    <div class="input-group">
                        <div class="input-group-btn">

                            <button type="button" class="btn btn-default dropdown-toggle droplist_company" data-toggle="dropdown">Company A <span class="caret"></span></button>
                            <ul class="dropdown-menu">
                                <li><a href="#">Company A</a></li>
                                <li><a href="#">Company B</a></li>
                                <li><a href="#">Company C</a></li>
                            </ul>
                        </div><!-- /btn-group -->

                        <input type="text" class="form-control" placeholder="find report or chart">
                    </div><!-- /input-group -->
                </div>

            </div>
        </div>

        <div class="col-md-1 user_home_divider user_chart_box">
            <div class="row">
                <div class="col-md-12 ">
                    <span class="user_chart_vs"> Compare Data</span>
                </div>
            </div>
        </div>

        <div class="col-md-2 user_home_divider user_chart_box">
            <div class="row">
                <div class="col-md-12 ">
                    <span class="user_clock user_chart_clock"><strong>1h22mins left</strong> for submitting decision</span>
                </div>
            </div>
        </div>

        <div class="col-md-2">
            <div class="row">
                <div class="col-md-12 ">
                    <span class="user_chart_make_decision">Mission: Market Share + Profit</span>
                    <button type="button" class="btn user_chart_decision_button">Make Decision</button>
                </div>
            </div>

        </div>

    </div>

    <div class="row mainbody bgbrand" ng-if="css.menu=='Report'">
        <div class="col-md-2 user_chart_padding user_home_divider">

            <div class="panel user_chart_leftmenu">
                <div class="panel-heading">Your Company</div>

                <!-- List group -->
                <ul class="list-group">
                    <li class="list-group-item" ng-class="{'list-group-item-active': css.chartMenu=='A1'}" ng-click="clickChartMenu('A1')">Company Status</li>
                    <li class="list-group-item" ng-class="{'list-group-item-active': css.chartMenu=='A2'}" ng-click="clickChartMenu('A2')">Financial Data</li>
                    <li class="list-group-item" ng-class="{'list-group-item-active': css.chartMenu=='A3'}" ng-click="clickChartMenu('A3')">Inventory Report</li>
